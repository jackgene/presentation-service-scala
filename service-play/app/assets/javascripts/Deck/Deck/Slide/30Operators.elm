-- Constants
heading : String
heading = "An Overview of Reactive Streams"


-- Slides
operator : String -> Html Msg -> Operand -> Operation -> Operand -> Bool -> Bool -> UnindexedSlideModel
operator subheading textView input operation output showCode animate =
  { baseSlideModel
  | view =
    ( \page _ ->
      standardSlideView page heading subheading
      ( div [ css [ position absolute ] ]
        [ div
          [ css [ position absolute, left zero, width (vw 46) ] ]
          [ textView ]
        , div -- diagram frame
          [ css [ position absolute, left (vw 46), width (vw 40) ] ]
          [ diagramView input operation output animate
          ]
        ]
      )
    )
  }


